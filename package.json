{
    "name": "@rdfc/orchestrator-js",
    "version": "1.0.7",
    "main": "index.js",
    "type": "module",
    "bin": {
        "rdfc": "./bin/orchestrator.js"
    },
    "scripts": {
        "build": "tspc && tsc-alias",
        "lint": "eslint . --ext .ts",
        "format": "prettier --write .",
        "test": "npm run build && vitest run",
        "prepare": "husky"
    },
    "files": [
        "bin/**/*",
        "lib/**/*"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "echo \"Husky pre-commit\""
        }
    },
    "lint-staged": {
        "*.ts": [
            "eslint --fix",
            "prettier --write"
        ]
    },
    "keywords": [],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rdf-connect/orchestrator-js.git"
    },
    "author": "Arthur Vercruysse",
    "contributors": [
        "Ieben Smessaert"
    ],
    "license": "MIT",
    "description": "A JavaScript/TypeScript implementation of an RDF-based orchestrator for managing and executing data processing pipelines.",
    "devDependencies": {
        "@ajuvercr/ts-transformer-inline-file": "^0.2.0",
        "@eslint/js": "^9.21.0",
        "@rdfjs/types": "^2.0.1",
        "@types/jest": "^29.5.14",
        "@types/jsonld": "^1.5.15",
        "@types/n3": "^1.21.1",
        "@types/node": "^22.13.5",
        "@typescript-eslint/eslint-plugin": "^8.25.0",
        "@typescript-eslint/parser": "^8.25.0",
        "@vitest/coverage-v8": "^3.0.7",
        "eslint": "^9.21.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.3",
        "globals": "^16.0.0",
        "husky": "^9.1.7",
        "jest": "^29.7.0",
        "lint-staged": "^15.4.3",
        "prettier": "^3.5.2",
        "ts-jest": "^29.2.6",
        "ts-patch": "^3.3.0",
        "tsc-alias": "^1.8.10",
        "ttypescript": "^1.5.15",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.25.0",
        "vitest": "^3.0.7"
    },
    "peerDependencies": {
        "@grpc/grpc-js": "^1.12.6"
    },
    "dependencies": {
        "@grpc/grpc-js": "^1.12.6",
        "@rdfc/proto": "0.1.2-alpha-2",
        "@treecg/types": "^0.4.6",
        "jsonld-streaming-parser": "^5.0.0",
        "jszip": "^3.10.1",
        "n3": "^1.23.1",
        "rdf-lens": "^1.3.6",
        "winston": "^3.17.0",
        "winston-transport": "^4.9.0"
    }
}
