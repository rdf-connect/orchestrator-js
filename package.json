{
    "name": "@rdfc/orchestrator-js",
    "version": "2.0.1",
    "main": "index.js",
    "type": "module",
    "bin": {
        "rdfc": "./bin/orchestrator.js"
    },
    "scripts": {
        "build": "tspc && tsc-alias",
        "lint": "eslint . --ext .ts",
        "format": "prettier --write .",
        "test": "npm run build && vitest run",
        "prepare": "husky"
    },
    "files": [
        "bin/**/*",
        "lib/**/*"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "echo \"Husky pre-commit\""
        }
    },
    "lint-staged": {
        "*.ts": [
            "eslint --fix",
            "prettier --write"
        ]
    },
    "keywords": [],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rdf-connect/orchestrator-js.git"
    },
    "author": "Arthur Vercruysse",
    "contributors": [
        "Ieben Smessaert"
    ],
    "license": "MIT",
    "description": "A JavaScript/TypeScript implementation of an RDF-based orchestrator for managing and executing data processing pipelines.",
    "devDependencies": {
        "@ajuvercr/ts-transformer-inline-file": "^0.2.0",
        "@eslint/js": "^10.0.1",
        "@rdfjs/types": "^2.0.1",
        "@types/jest": "^30.0.0",
        "@types/jsonld": "^1.5.15",
        "@types/n3": "^1.26.1",
        "@types/node": "^24.10.13",
        "@typescript-eslint/eslint-plugin": "^8.55.0",
        "@typescript-eslint/parser": "^8.55.0",
        "@vitest/coverage-v8": "^4.0.18",
        "eslint": "^9.21.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.5",
        "globals": "^16.5.0",
        "husky": "^9.1.7",
        "jest": "^30.2.0",
        "lint-staged": "^16.2.7",
        "prettier": "^3.8.1",
        "ts-jest": "^29.4.6",
        "ts-patch": "^3.3.0",
        "tsc-alias": "^1.8.16",
        "ttypescript": "^1.5.15",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.55.0",
        "vitest": "^4.0.18"
    },
    "dependencies": {
        "@grpc/grpc-js": "^1.14.3",
        "@rdfc/proto": "^0.1.2",
        "@treecg/types": "^0.4.6",
        "jsonld-streaming-parser": "^5.0.1",
        "jszip": "^3.10.1",
        "n3": "^1.26.0",
        "rdf-lens": "^1.3.6",
        "winston": "^3.19.0",
        "winston-transport": "^4.9.0"
    }
}
